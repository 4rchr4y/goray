{
  "rust-analyzer.showUnlinkedFileNotification": false,
  "cSpell.words": [
    "ahash",
    "asynx",
    "bindgen",
    "bitflags",
    "cachedb",
    "canonicalize",
    "chrono",
    "dbutl",
    "disp",
    "ENOTDIR",
    "filetree",
    "gqlutl",
    "graphiql",
    "hashbrown",
    "Hasher",
    "inmem",
    "inmemdb",
    "jsonwebtoken",
    "keymap",
    "libc",
    "manifestfile",
    "mimalloc",
    "mlua",
    "MNID",
    "mosscore",
    "mossdev",
    "mossland",
    "oneshot",
    "otlp",
    "realfs",
    "repr",
    "revisioned",
    "rocksdb",
    "rootdb",
    "sched",
    "seaorm",
    "sess",
    "smol",
    "specta",
    "sqlx",
    "surql",
    "surrealdb",
    "tablename",
    "tauri",
    "thiserror",
    "unpark",
    "unparker",
    "unwatch",
    "worktree"
  ],
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "root",
      "group": "moss",
      "database": "${workspaceFolder:moss}/root.db"
    },
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "cache",
      "database": "${workspaceFolder:moss}/.moss/cache/cache.db",
      "group": "moss"
    }
  ],
  "sqltools.useNodeRuntime": true,
  "todohighlight.keywords": [
    {
      "text": "NOTE:",
      "color": "#171717",
      "backgroundColor": "#f5f5f4"
    },
    {
      "text": "TODO:",
      "color": "white",
      "backgroundColor": "#facc15"
    },
    {
      "text": "OPTIMIZE:",
      "color": "white",
      "backgroundColor": "#d97706"
    },
    {
      "text": "BUG:",
      "color": "white",
      "backgroundColor": "#b91c1c"
    },
    {
      "text": "FIXME:",
      "color": "white",
      "backgroundColor": "#f87171"
    },
    {
      "text": "HACK:",
      "color": "white",
      "backgroundColor": "#9333ea",
      "isWholeLine": false
    }
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
}